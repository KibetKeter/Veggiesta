<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.veggiesta">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Cart">
        </activity>
        <activity android:name=".Sign_up">
        </activity>
        <activity android:name=".Authentication">
        </activity>
        <activity android:name=".Market">
        </activity>
        <activity
                android:name=".ui.login.SignUp"
                android:label="@string/title_activity_sign_up">
        </activity>
        <activity
                android:name=".ui.login.Sign_up_activity"
                android:label="@string/title_activity_sign_up_activity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts"/>
    </application>

</manifest>