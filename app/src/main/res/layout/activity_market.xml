<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Market">
    <View
            android:id="@+id/rectangle_2"
            android:layout_width="0dp"
            android:layout_height="82dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="-1dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="-24dp"
            android:background="#52d66f"
            tools:ignore="MissingConstraints"/>

    <TextView
            android:text="Popular Meals"
            android:layout_width="130dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:id="@+id/popular_meal"
            tools:layout_editor_absoluteY="38dp"
            tools:layout_editor_absoluteX="16dp"
            tools:ignore="MissingConstraints"
            android:textAppearance="@style/popular_mea"
            android:gravity="top" android:textStyle="bold" android:fontFamily="@font/dosis_bold"/>
    <View
            android:id="@+id/vector"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:background="@drawable/vector"
            tools:ignore="MissingConstraints"
    />
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_vector"
            android:id="@+id/imageButton"
            tools:layout_editor_absoluteY="43dp"
            tools:layout_editor_absoluteX="267dp"
            tools:ignore="MissingConstraints,VectorDrawableCompat"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_male"
            android:id="@+id/imageButton3"
            tools:layout_editor_absoluteY="43dp"
            tools:ignore="MissingConstraints,VectorDrawableCompat"
            tools:layout_editor_absoluteX="329dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>